{
  "name": "pangea-node-sdk",
  "version": "5.2.0",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": "github:pangeacyber/pangea-javascript",
  "author": "Glenn Gallien <glenn.gallien@pangea.cloud>",
  "license": "MIT",
  "engines": {
    "node": "20 || >=22"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@aws-crypto/crc32c": "^5.2.0",
    "crypto-js": "^4.2.0",
    "form-data-encoder": "^4.1.0",
    "formdata-node": "^6.0.3",
    "json-canon": "^1.0.1",
    "merkle-tools": "^1.4.1",
    "promise-retry": "^2.0.1",
    "proper-url-join": "^2.1.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.1.3",
    "@stoplight/prism-cli": "5.14.2",
    "@tsconfig/node20": "20.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "20.19.9",
    "@types/promise-retry": "1.1.6",
    "@types/proper-url-join": "2.1.5",
    "cross-env": "^7.0.3",
    "publint": "0.3.12",
    "start-server-and-test": "2.0.13",
    "tsdown": "0.13.3",
    "typedoc": "^0.28.9",
    "typescript": "5.8.3",
    "ultracite": "5.1.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--openssl-legacy-provider vitest run",
    "test:unit": "yarn test tests/unit/",
    "test:integration": "yarn test tests/integration/",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "attw": "attw --pack",
    "publint": "publint --strict",
    "generate:docs": "typedoc --tsconfig ./tsconfig.prod.json",
    "build": "tsdown",
    "prepare": "yarn build"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
