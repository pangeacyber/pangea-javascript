{
  "name": "@pangeacyber/webcomponents",
  "version": "1.0.0",
  "author": "Michael Combs <michael.combs@pangea.cloud>",
  "license": "MIT",
  "type": "commonjs",
  "source": "src/main.ts",
  "main": "dist/lib/main.cjs",
  "module": "dist/lib/main.mjs",
  "types": "dist/lib/types.d.ts",
  "browser": {
    "audit-log-viewer": "dist/browser/audit-log-viewer.js",
    "share-file-viewer": "dist/browser/share-file-viewer.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "build": "parcel build",
    "lint-publint": "publint",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@parcel/packager-ts": "2.13.0",
    "@parcel/transformer-typescript-types": "2.13.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "parcel": "^2.13.0",
    "process": "^0.11.10",
    "publint": "0.2.12",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/styled-engine-sc": "^6.1.5",
    "@mui/types": "^7.2.18",
    "@pangeacyber/react-mui-audit-log-viewer": "^1.0.4",
    "@pangeacyber/react-mui-branding": "^1.0.0",
    "@pangeacyber/react-mui-share-file-viewer": "^1.0.2",
    "@preact/compat": "^18.3.1",
    "preact": "^10.24.3",
    "styled-components": "^6.1.13"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "targets": {
    "types": {},
    "main": {},
    "browser": false,
    "browsers": {
      "source": [
        "src/audit-log-viewer.tsx",
        "src/share-file-viewer.tsx"
      ],
      "distDir": "dist/browser",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "outputFormat": "global"
    }
  }
}
