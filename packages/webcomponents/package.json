{
  "name": "@pangeacyber/webcomponents",
  "version": "1.0.0",
  "author": "Michael Combs <michael.combs@pangea.cloud>",
  "license": "MIT",
  "type": "commonjs",
  "source": "src/main.ts",
  "main": "dist/lib/main.cjs",
  "module": "dist/lib/main.mjs",
  "types": "dist/lib/types.d.ts",
  "browser": {
    "audit-log-viewer": "dist/browser/audit-log-viewer.js",
    "share-file-viewer": "dist/browser/share-file-viewer.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "build": "parcel build",
    "lint-publint": "publint",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@parcel/packager-ts": "2.16.0",
    "@parcel/transformer-typescript-types": "2.16.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "parcel": "2.16.0",
    "publint": "0.3.12",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/styled-engine-sc": "^6.1.5",
    "@mui/types": "^7.2.18",
    "@pangeacyber/react-mui-audit-log-viewer": "^2.0.3",
    "@pangeacyber/react-mui-branding": "^1.0.0",
    "@pangeacyber/react-mui-share-file-viewer": "^2.0.2",
    "@preact/compat": "^18.3.1",
    "preact": "^10.27.2",
    "styled-components": "^6.1.19"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "targets": {
    "types": {},
    "main": {},
    "browser": false,
    "browsers": {
      "source": [
        "src/audit-log-viewer.tsx",
        "src/share-file-viewer.tsx"
      ],
      "distDir": "dist/browser",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "outputFormat": "global"
    }
  }
}
