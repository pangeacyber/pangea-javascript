{
  "name": "@pangeacyber/react-oauth",
  "version": "0.0.3-beta",
  "description": "React Auth Provider for Pangea OAuth",
  "type": "commonjs",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "author": "Glenn Gallien <glenn.gallien@pangea.cloud>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "rm -rf .parcel-cache && parcel build"
  },
  "devDependencies": {
    "@parcel/core": "^2.13.2",
    "@parcel/packager-ts": "^2.13.2",
    "@parcel/transformer-typescript-types": "2.13.2",
    "@tsconfig/strictest": "^2.0.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "parcel": "^2.13.2",
    "parcel-optimizer-react-use-client": "^1.0.0",
    "react": "^19.0.0",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "packageManager": "yarn@4.5.3",
  "dependencies": {
    "@pangeacyber/oauth-js": "0.0.3-beta"
  }
}
