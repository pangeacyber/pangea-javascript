{
  "name": "@pangeacyber/react-auth",
  "description": "Pangea auth provider React component",
  "version": "0.0.11",
  "type": "commonjs",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "repository": "https://github.com/pangeacyber/pangea-javascript.git",
  "author": "Pangea (https://pangea.cloud)",
  "license": "MIT",
  "scripts": {
    "build": "parcel build && cp dist/index.d.ts dist/index.d.mts",
    "lint-attw": "attw --pack",
    "lint-publint": "publint --strict",
    "prepack": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "jose": "^5.2.4",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@babel/core": "^7.24.4",
    "@parcel/config-default": "^2.12.0",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/plugin": "^2.12.0",
    "@parcel/source-map": "^2.1.1",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@parcel/utils": "^2.12.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.79",
    "parcel": "^2.12.0",
    "publint": "^0.2.7",
    "react": "^18.2.0",
    "typescript": "^5.4.2"
  }
}
